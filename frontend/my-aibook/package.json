{
  "name": "my-aibook",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "npx docusaurus build",
    "build:github": "node -e \"require('fs').writeFileSync('docusaurus.config.ts', require('fs').readFileSync('docusaurus.config.ts', 'utf8').replace(/baseUrl: '\\//g, 'baseUrl: \\'/december-aibook/'));process.exit(0);\" && npx docusaurus build && git checkout -- docusaurus.config.ts",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "process-docs": "node scripts/process-docs.js",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx src/",
    "lint:fix": "eslint --cache --ext .js,.jsx,.ts,.tsx src/ --fix",
    "format": "prettier --write --cache .",
    "format:check": "prettier --check --cache ."
  },
  "dependencies": {
    "@docusaurus/core": "3.9.2",
    "@docusaurus/preset-classic": "3.9.2",
    "@google/generative-ai": "^0.24.1",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.2",
    "@docusaurus/tsconfig": "3.9.2",
    "@docusaurus/types": "3.9.2",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0"
  }
}
